{
  "name": "js-html-parser",
  "version": "1.0.7",
  "description": "A very fast HTML parser, generating a simplified DOM, with basic element query support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha",
    "build": "node_modules/gulp/bin/gulp.js default",
    "pretest": "gulp",
    "prepare": "gulp"
  },
  "keywords": [
    "fast html parser nodejs typescript"
  ],
  "author": "Xiaoyi Shi <ashi009@gmail.com>",
  "contributors": [
    "taoqf<tao_qiufeng@126.com>",
    "Hoang Rio <donghoang.nguyen@outlook.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "he": "1.1.1"
  },
  "devDependencies": {
    "@types/entities": "latest",
    "@types/he": "latest",
    "@types/node": "latest",
    "blanket": "latest",
    "del": "latest",
    "gulp": "latest",
    "gulp-sequence": "latest",
    "gulp-shell": "latest",
    "gulp-typescript": "^5.0.1",
    "mocha": "latest",
    "should": "latest",
    "spec": "latest",
    "travis-cov": "latest",
    "typescript": "next"
  },
  "config": {
    "blanket": {
      "pattern": "./dist/index.js",
      "data-cover-never": [
        "node_modules"
      ]
    },
    "travis-cov": {
      "threshold": 70
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoang-rio/js-html-parser.git"
  },
  "bugs": {
    "url": "https://github.com/hoang-rio/js-html-parser/issues"
  },
  "homepage": "https://github.com/hoang-rio/js-html-parser"
}
